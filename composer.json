{
    "name": "flaxandteal/espyfm-php",
    "description": "EspyFM PHP is a client for EspyFM, itself a wrapper around Elasticsearch and Lyst's LightFM library",
    "type": "library",
    "keywords": ["lightfm", "recommendation"],
    "homepage": "https://github.com/flaxandteal/espyfm-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Phil Weir",
            "email": "phil.weir@flaxandteal.co.uk",
            "homepage": "https://flaxandteal.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/flaxandteal/espyfm-php/issues",
        "source": "https://github.com/flaxandteal/espyfm-php"
    },
    "require": {
        "php": ">=5.3.3",
        "phpunit/phpunit": "^8.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "satooshi/php-coveralls": "^2.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {"Flaxandteal\\EspyFM\\": "src/Flaxandteal/EspyFM/"}
    },
    "autoload-dev": {
        "psr-4": {"Flaxandteal\\EspyFM\\": "tests/Flaxandteal/EspyFM/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flaxandteal\\EspyFM\\EspyFMServiceProvider"
            ]
        }
    }
}
